      "@polkadot/app-tech-comm/*": [ "packages/page-tech-comm/src/*" ],
      "@polkadot/app-toolbox": [ "packages/page-toolbox/src" ],
      "@polkadot/app-toolbox/*": [ "packages/page-toolbox/src/*" ],
      "@polkadot/app-treasury": [ "packages/page-treasury/src" ],
